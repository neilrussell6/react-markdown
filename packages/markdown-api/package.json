{
  "name": "@react-markdown/markdown-api",
  "description": "A Markdown API for the React Web UI",
  "keywords": [
    "API",
    "express",
    "markdown",
    "javascript"
  ],
  "version": "0.0.0",
  "author": "Neil Russell <neilrussell6@gmail.com>",
  "license": "ISC",
  "homepage": "./",
  "scripts": {
    "serve": "dotenv -e .env -- nodemon src/index.js",
    "npm:audit": "audit-ci --critical",
    "code:format": "prettier --write .",
    "_code:lint:prettier": "prettier --check .",
    "_code:lint:eslint": "eslint .",
    "code:lint": "npm-run-all _code:lint:prettier _code:lint:eslint",
    "_git:pre-commit": "npm-run-all code:format code:lint",
    "_git:pre-push": "npm-run-all code:format code:lint",
    "init": "cp -n .env.tpl .env || true",
    "help": "package-json-help.js"
  },
  "engines": {
    "npm": "=6.14.4"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "express": "^4.17.1",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "audit": "npm:audit@0.0.6",
    "audit-ci": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^3.1.0",
    "eslint": "^7.4.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5"
  }
}
