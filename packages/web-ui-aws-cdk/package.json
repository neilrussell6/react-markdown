{
  "name": "@react-markdown/web-ui-aws",
  "description": "AWS CDK delivery package for React Markdown Web UI package",
  "keywords": [
    "react",
    "javascript"
  ],
  "version": "0.0.0",
  "author": "Neil Russell <neilrussell6@gmail.com>",
  "license": "ISC",
  "homepage": "./",
  "scripts": {
    "cdk:ls": "bin/cdk.sh ls",
    "_cdk:synth": "bin/cdk.sh synth",
    "cdk:synth": "npm-run-all code:build \"_cdk:synth {@}\" -- \"$@\"",
    "cdk:diff": "cdk diff",
    "cdk:bootstrap": "bin/cdk.sh bootstrap",
    "_cdk:deploy": "bin/cdk.sh deploy",
    "cdk:deploy": "npm-run-all code:build \"_cdk:deploy {@}\" -- \"$@\"",
    "cdk:destroy": "bin/cdk.sh destroy",
    "code:build": "tsc",
    "code:build:watch": "tsc -w",
    "test": "jest --ci --config jest.node.config.js --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "_test:all": "npm run test",
    "_ci:test": "CI=true npm run test",
    "_ci:test:all": "npm-run-all _ci:test",
    "code:format": "prettier --write \"src/**/*.ts\"",
    "_code:lint:prettier": "prettier --check \"src/**/*.ts\"",
    "_code:lint:eslint": "eslint . --ext ts",
    "code:lint": "npm-run-all _code:lint:prettier _code:lint:eslint",
    "_git:pre-commit": "CI=true npm-run-all code:format code:lint test",
    "_git:pre-push": "CI=true npm-run-all code:format code:lint _test:all",
    "sdk:cf": "bin/sdk.sh cf",
    "_init:env": "cp -n .env.tpl .env || true",
    "_init:env:dev": "cp -n .env.dev.tpl .env.dev || true",
    "_init:env:staging": "cp -n .env.staging.tpl .env.staging || true",
    "_init:env:production": "cp -n .env.production.tpl .env.production || true",
    "_init:envs": "npm-run-all _init:env _init:env:dev _init:env:staging _init:env:production",
    "help": "package-json-help.js"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.47.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^10.17.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "aws-cdk": "1.47.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.0",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "package-json-help": "0.0.10",
    "prettier": "^2.0.5",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.47.0",
    "@aws-cdk/aws-cloudfront": "1.47.0",
    "@aws-cdk/aws-ec2": "1.47.0",
    "@aws-cdk/aws-lambda": "1.47.0",
    "@aws-cdk/aws-lambda-nodejs": "1.47.0",
    "@aws-cdk/aws-s3-deployment": "1.47.0",
    "@aws-cdk/core": "1.47.0",
    "@react-markdown/web-ui": "^0.0.0",
    "@types/ramda": "^0.27.6",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.16"
  },
  "publishConfig": {
    "access": "private"
  }
}
