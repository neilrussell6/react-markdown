{
  "name": "@react-markdown/api-aws-cdk",
  "description": "AWS CDK delivery package for React Markdown API package",
  "keywords": [
    "AWS",
    "CDK",
    "API",
    "NodeJS"
  ],
  "version": "0.0.0",
  "author": "Neil Russell <neilrussell6@gmail.com>",
  "license": "ISC",
  "private": true,
  "scripts": {
    "cdk:ls": "bin/cdk.sh ls",
    "_cdk:synth": "bin/cdk.sh synth",
    "cdk:synth": "npm-run-all code:build \"_cdk:synth {@}\" -- \"$@\"",
    "cdk:diff": "cdk diff",
    "cdk:bootstrap": "bin/cdk.sh bootstrap",
    "_cdk:deploy": "bin/cdk.sh deploy",
    "cdk:deploy": "npm-run-all code:build code:bundle \"_cdk:deploy {@}\" -- \"$@\"",
    "cdk:destroy": "bin/cdk.sh destroy",
    "sdk:rds:ls": "bin/sdk.sh rds-ls",
    "code:build": "tsc",
    "code:build:watch": "tsc -w",
    "test": "jest --ci --config jest.node.config.js --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "_test:all": "npm run test",
    "_test:ci": "CI=true npm run test",
    "_test:all:ci": "npm run _test:ci",
    "code:format": "prettier --write \"src/**/*.ts\"",
    "_code:lint:prettier": "prettier --check \"src/**/*.ts\"",
    "_code:lint:eslint": "eslint  --ext ts",
    "code:lint": "npm-run-all _code:lint:prettier _code:lint:eslint",
    "code:bundle": "webpack --config webpack.config.js",
    "_git:pre-commit": "CI=true npm-run-all code:format code:lint code:build test code:bundle",
    "_git:pre-push": "CI=true npm-run-all code:format code:lint code:build _test:all code:bundle",
    "_init:env": "cp -n .env.tpl .env || true",
    "_init:env:dev": "cp -n .env.dev.tpl .env.dev || true",
    "_init:env:staging": "cp -n .env.staging.tpl .env.staging || true",
    "_init:env:production": "cp -n .env.production.tpl .env.production || true",
    "_init:envs": "npm-run-all _init:env _init:env:dev _init:env:staging _init:env:production",
    "init": "npm run _init:envs",
    "help": "package-json-help.js"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.47.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^10.17.26",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "aws-cdk": "^1.47.0",
    "aws-sdk": "^2.733.0",
    "copy-webpack-plugin": "^6.0.3",
    "dotenv-cli": "^3.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.1",
    "jest": "^25.5.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.10",
    "prettier": "^2.0.5",
    "tmp": "^0.2.1",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "yargs": "^15.4.1",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.47.0",
    "@aws-cdk/aws-ec2": "1.47.0",
    "@aws-cdk/aws-events": "1.47.0",
    "@aws-cdk/aws-events-targets": "1.47.0",
    "@aws-cdk/aws-lambda": "1.47.0",
    "@aws-cdk/aws-rds": "1.47.0",
    "@aws-cdk/aws-secretsmanager": "1.47.0",
    "@aws-cdk/aws-ssm": "1.47.0",
    "@aws-cdk/core": "1.47.0",
    "@react-markdown/api": "^0.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/ramda": "^0.27.6",
    "bluebird": "^3.7.2",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.16"
  },
  "publishConfig": {
    "access": "private"
  }
}
